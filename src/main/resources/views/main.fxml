<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0"
      xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.nrty.bulldog.interfaces.javafx.MainController">

    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="线性文件">
                    <items>
                        <MenuItem mnemonicParsing="false" text="平面线性文件"/>
                        <MenuItem mnemonicParsing="false" text="纵面线性文件"/>
                        <MenuItem mnemonicParsing="false" text="构造物类型文件"/>
                        <MenuItem mnemonicParsing="false" text="互通式立体交叉文件"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="公路等级">
                    <items>
                        <MenuItem mnemonicParsing="false" text="高级公路"/>
                        <MenuItem mnemonicParsing="false" text="一级公路"/>
                        <MenuItem mnemonicParsing="false" text="二级公路"/>
                        <MenuItem mnemonicParsing="false" text="三级公路"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="车型">
                    <items>
                        <MenuItem mnemonicParsing="false" text="小型车"/>
                        <MenuItem mnemonicParsing="false" text="大型车"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS"/>
    </children>
</VBox>
